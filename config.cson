"*":
  "exception-reporting":
    userId: "9271f751-9317-c1ec-03df-d92c59f8b44f"
  welcome:
    showOnStartup: false
  core:
    autoHideMenuBar: true
    ignoredNames: [
      ".git"
      "_core"
      "tmp"
      "dist"
      "node_modules"
      "bower_components"
    ]
    themes: [
      "one-dark-ui"
      "chester-atom-syntax"
    ]
    disabledPackages: [
      "metrics"
    ]
  editor:
    invisibles: {}
    softWrap: true
    fontSize: 20
    fontFamily: "Fira Code, monospace"
    softWrapHangingIndent: 20
  "go-plus":
    goPath: "~"
    goInstallation: "/usr/local/go"
    formatTool: "goreturns"
  "autocomplete-plus":
    confirmCompletion: "enter"
    backspaceTriggersAutocomplete: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  tabs:
    showIcons: true
    usePreviewTabs: true
  autosave:
    enabled: true
  "git-diff":
    showIconsInEditorGutter: true
"source.gfm":
  editor:
    softWrap: true
